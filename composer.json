{
    "name": "prinx/txtpay",
    "description": "TXTGHANA Payment Gateway SDK.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "prinx/dotenv": "^1.0",
        "prinx/notify": "^0.0",
        "symfony/http-client": "^5.1",
        "illuminate/collections": "^8.16",
        "symfony/http-foundation": "^5.1",
        "respect/validation": "^2.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4",
        "symfony/var-dumper": "^5.1",
        "nunomaduro/collision": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Txtpay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers" : [
                "Txtpay\\Laravel\\ServiceProvider"
            ]
        }
    }
}
